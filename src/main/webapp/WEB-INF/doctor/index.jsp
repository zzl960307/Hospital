        <th>备注</th>
        <th>操作</th>
    </tr>
		<c:forEach items="${doc }" var="m">
	     <tr>
         	<td style="vertical-align:middle;"><input type="checkbox" name="id" value="${m.id }"></td>
 				<td>${m.id}</td>
